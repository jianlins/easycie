<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
    <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
    <primitive>true</primitive>
    <annotatorImplementationName>edu.utah.bmi.nlp.easycie.writer.SQLWriterCasConsumer</annotatorImplementationName>
    <analysisEngineMetaData>
        <name>SQLWriter_AE</name>
        <description>Write annotations in structured format table</description>
        <configurationParameters>
            <configurationParameter>
                <name>DBConfigFile</name>
                <description>db configuration file
                </description>
                <type>String</type>
                <multiValued>false</multiValued>
                <mandatory>true</mandatory>
            </configurationParameter>
            <configurationParameter>
                <name>SnippetTableName</name>
                <description>The table name to save the snippet level annotations.
                </description>
                <type>String</type>
                <multiValued>false</multiValued>
                <mandatory>true</mandatory>
            </configurationParameter>
            <configurationParameter>
                <name>DocTableName</name>
                <description>The table name to save the document level annotations.
                </description>
                <type>String</type>
                <multiValued>false</multiValued>
                <mandatory>true</mandatory>
            </configurationParameter>
            <configurationParameter>
                <name>OverWriteTable</name>
                <description>If table exists, whether clean the table at first
                </description>
                <type>Boolean</type>
                <multiValued>false</multiValued>
                <mandatory>false</mandatory>
            </configurationParameter>
            <configurationParameter>
                <name>WriteConcepts</name>
                <description>The names of the concepts that need to be written to db
                </description>
                <type>String</type>
                <multiValued>false</multiValued>
                <mandatory>true</mandatory>
            </configurationParameter>
            <configurationParameter>
                <name>BatchSize</name>
                <description>Batch size for database writing
                </description>
                <type>Integer</type>
                <multiValued>false</multiValued>
                <mandatory>false</mandatory>
            </configurationParameter>
            <configurationParameter>
                <name>Annotator</name>
                <description>Annotator's name, or can be use to differentiate different version of runs
                </description>
                <type>String</type>
                <multiValued>false</multiValued>
                <mandatory>false</mandatory>
            </configurationParameter>
            <configurationParameter>
                <name>Version</name>
                <description>
                </description>
                <type>String</type>
                <multiValued>false</multiValued>
                <mandatory>false</mandatory>
            </configurationParameter>
            <configurationParameter>
                <name>MinTextLength</name>
                <description>
                </description>
                <type>Integer</type>
                <multiValued>false</multiValued>
                <mandatory>false</mandatory>
            </configurationParameter>
            <configurationParameter>
                <name>MaxSnippetLength</name>
                <description>
                </description>
                <type>Integer</type>
                <multiValued>false</multiValued>
                <mandatory>false</mandatory>
            </configurationParameter>
        </configurationParameters>
        <configurationParameterSettings>
            <nameValuePair>
                <name>DBConfigFile</name>
                <value>
                    <string>data/output.sqlite</string>
                </value>
            </nameValuePair>
            <nameValuePair>
                <name>SnippetTableName</name>
                <value>
                    <string>RESULT_SNIPPET</string>
                </value>
            </nameValuePair>
            <nameValuePair>
                <name>DocTableName</name>
                <value>
                    <string>RESULT_DOC</string>
                </value>
            </nameValuePair>
            <nameValuePair>
                <name>OverWriteTable</name>
                <value>
                    <boolean>false</boolean>
                </value>
            </nameValuePair>
            <nameValuePair>
                <name>WriteConcepts</name>
                <value>
                    <string>Concept,Doc_Base</string>
                </value>
            </nameValuePair>
            <nameValuePair>
                <name>BatchSize</name>
                <value>
                    <integer>150</integer>
                </value>
            </nameValuePair>
            <nameValuePair>
                <name>Annotator</name>
                <value>
                    <string></string>
                </value>
            </nameValuePair>
            <nameValuePair>
                <name>Version</name>
                <value>
                    <string></string>
                </value>
            </nameValuePair>
            <nameValuePair>
                <name>MinTextLength</name>
                <value>
                    <integer>0</integer>
                </value>
            </nameValuePair>
            <nameValuePair>
                <name>MaxSnippetLength</name>
                <value>
                    <integer>10000</integer>
                </value>
            </nameValuePair>
        </configurationParameterSettings>
        <typeSystemDescription>
            <imports>
                <import location="../type/All_Types.xml"/>
            </imports>
        </typeSystemDescription>
        <capabilities>
            <capability>
                <inputs/>
                <outputs/>
                <languagesSupported/>
            </capability>
        </capabilities>
        <operationalProperties>
            <modifiesCas>true</modifiesCas>
            <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
            <outputsNewCASes>false</outputsNewCASes>
        </operationalProperties>
    </analysisEngineMetaData>
</analysisEngineDescription>
